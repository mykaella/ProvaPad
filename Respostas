// Questão 2

var someFn = função (numero) 

    {

        var valor = numero;

        função de retorno (incremento)

        {

            valor + = incremento;

            valor de retorno;

        }

    }

    var contador = someFn (1);

    console.log ("Primeira chamada", contador (3));

    console.log ("segunda chamada", contador (1));

    console.log ("Terceira chamada", contador (5));

